@import "/plugin/css/animate.wxss";

.top_tip{
  margin-top: 70rpx;
}
.top_tip>text{
  font-size: 18px;
}
.phone_choose_item{
  display: flex;
  align-items: center;
  flex-direction: column;
  margin: 100rpx auto;
  color:#666;
}
.phone_choose_item .phone_icon{
  width:100rpx;
  height:100rpx;
  border-radius : 50%;
  margin-bottom: 20rpx;
}
.phone_choose_item .phone_icon>image{
  width:100%;
  height:100%;
}